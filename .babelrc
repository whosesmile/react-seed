{
  presets: ['env', 'react'],
  "plugins": [
    "syntax-dynamic-import",
    "transform-class-properties",
    "transform-decorators-legacy",
    "transform-object-rest-spread",
    ["transform-runtime", {
      "helpers": true,
      "polyfill": true,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ]
}
